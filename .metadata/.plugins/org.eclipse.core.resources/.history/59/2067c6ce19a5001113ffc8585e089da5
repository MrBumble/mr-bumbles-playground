	}

	public double encrypt(double number, double exp, double N) {
		double code;
		//String clean = text;
		//int length = clean.length();
		
		//for(int i = 0; i < length; i++){
			code = Math.pow(number,exp) % N;
	
		//}

		return code;

	}

	public String decrypt() {
		String clean = "";

		return clean;

	}
	
	
	public static void main(String[] args){
		RSA rsa = new RSA();
		System.out.println(rsa.encrypt(7, 23, 143));
		System.out.println(Math.pow(7, 23)% 143);
	}
}
